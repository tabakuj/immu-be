

BE:
cd /Users/user/projects/juligo/immuSolution/immudb

docker build  -t immudb-docker-img --build-arg FILEDIR="./config" --build-arg FILENAME="default.yaml" --build-arg TARGETFILENAME="default.yaml" .
docker rm -f immuapi
docker run --name immuapi -p 8080:8080 -it immudb-docker-img

FE:

cd /Users/user/projects/juligo/immuSolution/immufe

docker build --build-arg NEXT_PUBLIC_API_URL="http://host.docker.internal:8080" -t  immu-solution-fe .
docker rm -f imu-fe
docker run -it -p 8081:3000 --name imu-fe immu-solution-fe



--BOTH
 docker compose up -d
