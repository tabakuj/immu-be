        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/v1/api",
    "paths": {
        "/account-info": {
            "get": {
                "description": "Retrieve a paginated list of all account information",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Get all account infos",
                "operationId": "get-all-account-infos",
                "parameters": [
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "Page number",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 10,
                        "description": "Page size",
                        "name": "pageSize",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of account info",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-array_internal_handlers_AccountInfoDto"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    }
                }
            },
            "post": {
                "description": "Add a new account information entry",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Create account info",
                "operationId": "create-account-info",
                "parameters": [
                    {
                        "description": "Account information",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.AccountInfoDto"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created account info",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-internal_handlers_AccountInfoDto"
                        }
                    },
                    "400": {
                        "description": "Bad request, invalid input",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    }
                }
            }
        },
        "/account-info/{id}": {
            "get": {
                "description": "Retrieve account information for a specific account by ID",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Get account info by ID",
                "operationId": "get-account-info-by-id",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Account ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Account information",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-internal_handlers_AccountInfoDto"
                        }
                    },
                    "400": {
                        "description": "Bad request, ID is required",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    }
                }
            },
            "put": {
                "description": "Update account information for a specific account by ID (currently not allowed)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Update account info",
                "operationId": "update-account-info",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Account ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Updated account information",
                        "name": "account",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.AccountInfoDto"
                        }
                    }
                ],
                "responses": {
                    "405": {
                        "description": "Method not allowed",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    }
                }
            },
            "delete": {
                "description": "Delete account information for a specific account by ID (currently not allowed)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Delete account info",
                "operationId": "delete-account-info",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Account ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "405": {
                        "description": "Method not allowed",
                        "schema": {
                            "$ref": "#/definitions/internal_handlers.Response-string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "immudb_internal_models.AccountType": {
            "type": "integer",
            "enum": [
                1,
                2
            ],
            "x-enum-comments": {
                "Receiving": "2",
                "Sending": "1"
            },
            "x-enum-varnames": [
                "Sending",
                "Receiving"
            ]
        },
