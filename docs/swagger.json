                    "type": "string"
                },
                "account_number": {
                    "type": "integer"
                },
                "address": {
                    "type": "string"
                },
                "amount": {
                    "type": "number"
                },
                "iban": {
                    "type": "string"
                },
                "type": {
                    "$ref": "#/definitions/immudb_internal_models.AccountType"
                }
            }
        },
        "internal_handlers.Response-array_internal_handlers_AccountInfoDto": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/internal_handlers.AccountInfoDto"
                    }
                },
                "error_message": {
                    "type": "string"
                }
            }
        },
        "internal_handlers.Response-internal_handlers_AccountInfoDto": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/internal_handlers.AccountInfoDto"
                },
                "error_message": {
                    "type": "string"
                }
            }
        },
        "internal_handlers.Response-string": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string"
                },
                "error_message": {
                    "type": "string"
                }
            }
        }
    },
    "externalDocs": {
