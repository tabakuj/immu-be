{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample server that connects with immudb vault stores and receives data from there.",
        "title": "Immudb Sample",
        "contact": {
            "name": "Julian Tabaku",
            "email": "julian.tabaku@outlook.com"
        },
        "license": {
            "name": "Apache 2.0",
